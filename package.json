{
  "name": "react-toastnotes",
  "version": "3.3.0-apha",
  "description": "Flexible, easy to implement toast notifications for react",
  "main": "commonjs/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "style": "src/styles.css",
  "scripts": {
    "build-library-commonjs": "rimraf ./commonjs && tsc -p tsconfig.lib.json --module commonjs --outDir commonjs",
    "build-library": "rimraf ./lib && tsc -p tsconfig.lib.json --outDir ./lib",
    "prepublishOnly": "yarn run build-library && yarn run build-library-commonjs"
  },
  "dependencies": {
    "@reach/alert": "0.12.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react-spring": "^9.0.0-rc.3"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",

    "react-scripts": "^4.0.1",

    "rimraf": "^2.6.3",
    "typescript": "^4.1.3"
  },
  "author": "heerzhang <herzhang@163.com>",
  "homepage": "https://github.com/heerzhang/react-toastnotes",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component",
    "toast",
    "notifications"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
